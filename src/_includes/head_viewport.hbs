{{#is website.meta.viewport.type "liquid" ~}}
<meta name="viewport" content="width=device-width, initial-scale=1">
{{/is ~}}
{{#is website.meta.viewport.type "noScale" ~}}
<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, minimum-scale=1, maximum-scale=1">
{{/is ~}}
{{#is website.meta.viewport.type "fixed" ~}}
<meta name="viewport" content="width={{website.meta.viewport.width}}">
{{/is ~}}